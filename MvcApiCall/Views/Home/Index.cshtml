<h1>All Articles:</h1>

<ol>
@foreach (Article article in @Model)
{
    <ul>
        <li>Section: @article.Section</li>
        <li>Title: @article.Title</li>
        <li>Abstract: @article.Abstract</li>
        <li>Url:<a href="@article.Url"> @article.Url</a></li>
        <li>Byline: @article.Byline</li>
    </ul>
    <br>
}
</ol>